<apex:page>
  <div id="angularApp"></div>
  <script type="text/javascript">
    // Dynamically load the Angular app's main.js without hardcoding the resource ID
    var angularAppResourceUrl = "{!URLFOR($Resource.MyAngularApp)}"; // This dynamically resolves the Static Resource URL

    // Dynamically load the Angular main.js file
    var script = document.createElement("script");
    script.src = angularAppResourceUrl + "/main-XACKM5CF.js"; // Adjust path if needed
    script.onload = function () {
      console.log("Angular app loaded.");
    };
    script.onerror = function (error) {
      console.error("Error loading Angular app script:", error);
    };
    document.getElementById("MyAngularApp").appendChild(script);

    // Load CSS
    var cssLink = document.createElement("link");
    cssLink.rel = "stylesheet";
    cssLink.href = angularAppResourceUrl + "/styles-5INURTSO.css"; // Adjust path if needed
    cssLink.onload = function () {
      console.log("CSS loaded successfully.");
    };
    cssLink.onerror = function (error) {
      console.error("Error loading CSS:", error);
    };
    document.head.appendChild(cssLink);
  </script>
</apex:page>
